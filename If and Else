public static void main(String[] args) {
         int x=4;
        if(x==3){
            System.out.println("jika if nilai x = 1");
        }
        else if(x==2){
            System.out.println("jika else if nilai x = 2");
        }else if(x==3){
            System.out.println("jika else ifnilai x = 3");
        }else{
            System.out.println("nilai x tidak diketahui jika x meelebihi 3");
    }
    
